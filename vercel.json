{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "env": {
    "YOUR_ENV_VARIABLE": "YOUR_VALUE"
  }
}
